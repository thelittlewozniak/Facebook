<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Register</servlet-name>
        <servlet-class>Controller.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MakeAPost</servlet-name>
        <servlet-class>Controller.PostServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LikeAPost</servlet-name>
        <servlet-class>Controller.LikePostServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MakeAComment</servlet-name>
        <servlet-class>Controller.MakeCommentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnLikeAPost</servlet-name>
        <servlet-class>Controller.UnLikePostServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LikeAComment</servlet-name>
        <servlet-class>Controller.LikeCommentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UnLikeAComment</servlet-name>
        <servlet-class>Controller.UnLikeCommentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogOut</servlet-name>
        <servlet-class>Controller.LogOutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WorkAdd</servlet-name>
        <servlet-class>Controller.WorkAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SchoolAdd</servlet-name>
        <servlet-class>Controller.SchoolAddServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Index</servlet-name>
        <jsp-file>/views/Index/index.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Activity</servlet-name>
        <jsp-file>/views/Home/index.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <jsp-file>/views/Home/search.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>Profile</servlet-name>
        <jsp-file>/views/Profile/profile.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>ProfileFriend</servlet-name>
        <jsp-file>/views/Profile/ProfileFriend.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>AddAWork</servlet-name>
        <jsp-file>/views/Profile/AddAWork.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>AddASchool</servlet-name>
        <jsp-file>/views/Profile/AddASchool.jsp</jsp-file>
    </servlet>
    <servlet>
        <servlet-name>EditDetails</servlet-name>
        <jsp-file>/views/Profile/EditDetails.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/Index/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/Login/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Activity</servlet-name>
        <url-pattern>/Activity/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Register</servlet-name>
        <url-pattern>/Register/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MakeAPost</servlet-name>
        <url-pattern>/MakeAPost/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LikeAPost</servlet-name>
        <url-pattern>/LikeAPost/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnLikeAPost</servlet-name>
        <url-pattern>/UnLikeAPost/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MakeAComment</servlet-name>
        <url-pattern>/MakeAComment/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LikeAComment</servlet-name>
        <url-pattern>/LikeAComment/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnLikeAComment</servlet-name>
        <url-pattern>/UnLikeAComment/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogOut</servlet-name>
        <url-pattern>/LogOut/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Profile</servlet-name>
        <url-pattern>/Profile/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileFriend</servlet-name>
        <url-pattern>/ProfileFriend/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddAWork</servlet-name>
        <url-pattern>/AddAWork/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WorkAdd</servlet-name>
        <url-pattern>/WorkAdd/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddASchool</servlet-name>
        <url-pattern>/AddASchool/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SchoolAdd</servlet-name>
        <url-pattern>/SchoolAdd/</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditDetails</servlet-name>
        <url-pattern>/EditDetails/*</url-pattern>
    </servlet-mapping>
</web-app>